{
  "name": "dogehouse",
  "private": true,
  "workspaces": {
    "packages": [
      "kebab",
      "kibbeh",
      "dinner",
      "dolma"
    ],
    "nohoist": [
      "**"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jest": "^26.0.23",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^6.0.0",
    "mediasoup-client": "^3.6.30",
    "mediasoup-client-aiortc": "^3.6.3",
    "ts-jest": "^26.5.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "defaultScope": "global"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "commit": "cz",
    "commit:signed": "cz -S"
  }
}
