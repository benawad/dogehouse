  "scripts": {
    "storybook": "start-storybook -p 9000 -c .storybook",
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true craco start",
    "start:staging": "cross-env SKIP_PREFLIGHT_CHECK=true REACT_APP_IS_STAGING=true REACT_APP_API_BASE_URL=https://doge-staging.stripcode.dev craco start",
    "gen:i18:keys": "ts-node --project scripts/tsconfig.json scripts/generateTranslationTypes.ts",
    "sync:i18": "ts-node --project scripts/tsconfig.json scripts/syncTranslations.ts",
    "i18": "npm run gen:i18:keys && npm run sync:i18",
    "build": "craco build",
    "test": "craco test",
    "vscode": "webpack --config src/app/webpack.config.js --watch --mode development",
    "vscode:prod": "webpack --config src/app/webpack.config.js --mode production",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
